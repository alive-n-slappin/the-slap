          <playerInfo>
            <viralPlayerName>viaviralkids</viralPlayerName>
            <partnerPlayerName>viaviralkids</partnerPlayerName>
            <networkPlayerName>viaviralkids</networkPlayerName>
          </playerInfo>

          <googleReporter enabled='false'>
